<% include ../partials/header %>
<div class="jumbotron">
    <table class="table">     
        <thead>
            <tr>
                <th>ID</th>
                <th>Title</th>
                <th>Price</th>
    
                
    
            </tr>
        </thead>
        <tbody id="myTable">
            <%products.forEach(function(product){ %>
            <tr>
                <td scope="row"> <%= product.id %></td>
                <td><%= product.title %></td>
                <td><%= product.price %></td>
             
            </tr>
            <% }); %>
        </tbody>
    </table>

    </div>




<% include ../partials/footer %>